{
    "manifest_version": 3,
    "name": "Superior Alphabet",
    "description": "Provides an improved alphabet",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
        "16": "icon-16.png",
        "32": "icon-16.png",
        "48": "icon-16.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://*/*"
          ]
        }
      ]
}